                <Label layoutX="265.0" layoutY="20.0" text="Connected Territory" />
                <TextField fx:id="TerritoryArmy" editable="false" layoutX="77.0" layoutY="254.0" prefHeight="31.0" prefWidth="92.0" />
                <Text layoutX="31.0" layoutY="275.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Army" />
